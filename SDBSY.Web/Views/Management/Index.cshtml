@model AdminUserDTO
<!DOCTYPE HTML>
<html>
<head>
    <meta charset="utf-8">
    <meta name="renderer" content="webkit|ie-comp|ie-stand">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" />
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <LINK rel="Bookmark" href="/favicon.ico">
    <LINK rel="Shortcut Icon" href="/favicon.ico" />
    <!--[if lt IE 9]>
    <script type="text/javascript" src="~/content/lib/html5.js"></script>
    <script type="text/javascript" src="~/content/lib/respond.min.js"></script>
    <script type="text/javascript" src="~/content/lib/PIE_IE678.js"></script>
    <![endif]-->
    <link href="~/content/css/H-ui.min.css" rel="stylesheet" type="text/css" />
    <link href="~/content/css/H-ui.admin.css" rel="stylesheet" type="text/css" />
    <link href="~/content/skin/default/skin.css" rel="stylesheet" type="text/css" id="skin" />
    <link href="~/content/lib/Hui-iconfont/1.0.1/iconfont.css" rel="stylesheet" type="text/css" />
    <link href="~/content/css/style.css" rel="stylesheet" type="text/css" />
    <!--[if IE 6]>
    <script type="text/javascript" src="http://lib.h-ui.net/DD_belatedPNG_0.0.8a-min.js" ></script>
    <script>DD_belatedPNG.fix('*');</script>
    <![endif]-->
    <title>幼儿信息管理平台</title>
</head>
<body>
    <header class="Hui-header cl">
        <a class="Hui-logo l" title="滨州市实验幼儿园" href="/">滨州市实验幼儿园</a> <a class="Hui-logo-m l" href="/" title="幼儿信息管理平台">幼儿信息管理平台</a> <span class="Hui-subtitle l">V1.0</span>
        <nav class="mainnav cl" id="Hui-nav">
            <ul>
                <li class="dropDown dropDown_click">

                </li>
            </ul>
        </nav>
        <ul class="Hui-userbar">
            <li>账号</li>
            <li class="dropDown dropDown_hover">
                <a href="#" class="dropDown_A">@Model.UserName <i class="Hui-iconfont">&#xe6d5;</i></a>
                <ul class="dropDown-menu radius box-shadow">
                    <li><a href="javascript:;" onclick="changepass()">修改密码</a></li>
                    <li><a href="javascript:;" onclick="logout()">退出</a></li>
                </ul>
            </li>
            <li id="Hui-skin" class="dropDown right dropDown_hover">
                <a href="javascript:;" title="换肤"><i class="Hui-iconfont" style="font-size:18px">&#xe62a;</i></a>
                <ul class="dropDown-menu radius box-shadow">
                    <li><a href="javascript:;" data-val="default" title="默认（黑色）">默认（黑色）</a></li>
                    <li><a href="javascript:;" data-val="blue" title="蓝色">蓝色</a></li>
                    <li><a href="javascript:;" data-val="green" title="绿色">绿色</a></li>
                    <li><a href="javascript:;" data-val="red" title="红色">红色</a></li>
                    <li><a href="javascript:;" data-val="yellow" title="黄色">黄色</a></li>
                    <li><a href="javascript:;" data-val="orange" title="绿色">橙色</a></li>
                </ul>
            </li>
        </ul>
        <a aria-hidden="false" class="Hui-nav-toggle" href="#"></a>
    </header>
    <aside class="Hui-aside">
        <div class="menu_dropdown bk_2">
            
                <dl id="menu-member" class="@Html.RouteIf("index","active")">
                    <dt><i class="Hui-iconfont">&#xe60d;</i> 幼儿管理<i class="Hui-iconfont menu_dropdown-arrow">&#xe6d5;</i></dt>
                    <dd>
                        <ul>
                            <li><a _href="/Student/NewList" href="javascript:;">新报名幼儿</a></li>
                            <li><a _href="/Student/UnPassList" href="javascript:;">报名未通过幼儿</a></li>
                            <li><a _href="/Student/List?classId=0" href="javascript:;">在园幼儿</a></li>
                            <li><a _href="/Student/BiYeList" href="javascript:;">已毕业幼儿</a></li>
                        </ul>
                    </dd>
                </dl>
                <dl id="menu-admin">
                    <dt><i class="Hui-iconfont">&#xe62d;</i> 教师管理<i class="Hui-iconfont menu_dropdown-arrow">&#xe6d5;</i></dt>
                    <dd>
                        <ul>
                            <li><a _href="/Teacher/List" href="javascript:void(0)">教师列表</a></li>
                        </ul>
                    </dd>
                </dl>

            <dl id="menu-admin">
                <dt><i class="Hui-iconfont">&#xe611;</i> 班级管理<i class="Hui-iconfont menu_dropdown-arrow">&#xe6d5;</i></dt>
                <dd>
                    <ul>
                        <li><a _href="/Class/List" href="javascript:void(0)">班级列表</a></li>
                    </ul>
                </dd>
            </dl>
            <dl id="menu-admin">
                <dt><i class="Hui-iconfont">&#xe6ca;</i> 票据管理<i class="Hui-iconfont menu_dropdown-arrow">&#xe6d5;</i></dt>
                <dd>
                    <ul>
                        <li><a _href="/Invoice/List" href="javascript:void(0)">票据管理</a></li>
                    </ul>
                </dd>
            </dl>
            
                <dl id="menu-admin">
                    <dt><i class="Hui-iconfont">&#xe643;</i> 后勤管理<i class="Hui-iconfont menu_dropdown-arrow">&#xe6d5;</i></dt>
                    <dd>
                        <ul>
                            <li><a _href="/Food/List" href="javascript:void(0)">食材管理</a></li>
                            <li><a _href="/Food/BuyRecordsList/0" href="javascript:void(0)">食材入库记录</a></li>
                        </ul>
                    </dd>
                </dl>
            
                <dl id="menu-system">
                    <dt><i class="Hui-iconfont">&#xe62e;</i> 系统管理<i class="Hui-iconfont menu_dropdown-arrow">&#xe6d5;</i></dt>
                    <dd>
                        <ul>

                            <li><a _href="/Role/List" href="javascript:void(0)">角色管理</a></li>
                            <li><a _href="/Permission/List" href="javascript:void(0)">权限管理</a></li>
                            <li><a _href="/AdminUser/List" href="javascript:void(0)">账号管理</a></li>
                            <li><a _href="/User/List" href="javascript:void(0)">用户管理</a></li>
                            <li><a _href="/System/Index" href="javascript:void(0)">系统设置</a></li>
                            <li><a _href="/AdminLog/List" href="javascript:void(0)">系统日志</a></li>
                        </ul>
                    </dd>
                </dl>
        </div>
    </aside>
    <div class="dislpayArrow"><a class="pngfix" href="javascript:void(0);" onClick="displaynavbar(this)"></a></div>
    <section class="Hui-article-box">
        <div id="Hui-tabNav" class="Hui-tabNav">
            <div class="Hui-tabNav-wp">
                <ul id="min_title_list" class="acrossTab cl">
                    <li class="active"><span title="我的桌面" data-href="~/Management/Welcome">我的桌面</span><em></em></li>
                </ul>
            </div>
            <div class="Hui-tabNav-more btn-group"><a id="js-tabNav-prev" class="btn radius btn-default size-S" href="javascript:;"><i class="Hui-iconfont">&#xe6d4;</i></a><a id="js-tabNav-next" class="btn radius btn-default size-S" href="javascript:;"><i class="Hui-iconfont">&#xe6d7;</i></a></div>
        </div>
        <div id="iframe_box" class="Hui-article">
            <div class="show_iframe">
                <div style="display:none" class="loading"></div>
                <iframe scrolling="yes" frameborder="0" src="~/Management/Welcome"></iframe>
            </div>
        </div>
    </section>
    <script type="text/javascript" src="~/content/lib/jquery/1.9.1/jquery.min.js"></script>
    <script type="text/javascript" src="~/content/lib/layer/1.9.3/layer.js"></script>
    <script type="text/javascript" src="~/content/js/H-ui.js"></script>
    <script type="text/javascript" src="~/content/js/H-ui.admin.js"></script>
    <script type="text/javascript">
        function logout() {
            layer.confirm("确定要退出系统吗？", function () {
                window.location.href = "/Management/Logout";
            });
        }
        function changepass() {
            layer_show("修改密码", "/Management/ChangePassword");
        }
    </script>
</body>
</html>